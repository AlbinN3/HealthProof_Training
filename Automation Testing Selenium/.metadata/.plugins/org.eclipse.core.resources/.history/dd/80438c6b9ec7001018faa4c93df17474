package stepdefenition;

import io.cucumber.java.en.Given;
import io.cucumber.java.en.Then;
import io.cucumber.java.en.When;

public class testcase1_homePage {
	@Given("the user in the HOME page")
	public void the_user_in_the_HOME_page() {
	    System.out.println("the user in home");
	}
	@When("he launches the HOME page")
	public void he_launches_the_HOME_page() {
	    System.out.println("When hr launches the home page");
	}
	@Then("he should be able to view the links")
	public void he_should_be_able_to_view_the_links() {
	    System.out.println("Able to view links");
	}
}
